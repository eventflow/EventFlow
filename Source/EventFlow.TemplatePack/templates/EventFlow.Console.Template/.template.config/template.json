{
  "$schema": "http://json.schemastore.org/template",
  "author": "Rasmus Mikkelsen",
  "classifications": [ "Console" ],
  "name": "EventFlow Console Application Template",
  "identity": "EventFlow.Console.Template.CSharp",
  "groupIdentity": "EventFlowConsole",
  "shortName": "eventflowapp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "EventFlow.Console.Template",
  "preferNameDirectory": true,
  "symbols": {
