      "replaces": "Entity",
      "FileRename": "Entity"
    }
  },
  "sources":[{
    "modifiers": [
      {
        "exclude": [ ".vs/**" ]
      },
      {
        "exclude": [ "**/obj/**/*", "**/bin/**/*", "**/.git/**/*" ]
      }
    ]
  }]
}